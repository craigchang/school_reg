<h2>Parent Information</h2>

<%= f.fields_for :parents do |f_parent| %>
  <div class="form-group">
    <%= f_parent.label :first_name %><br>
    <%= f_parent.text_field :first_name, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f_parent.label :last_name %><br>
    <%= f_parent.text_field :last_name, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f_parent.label :occupation %><br>
    <%= f_parent.text_field :occupation, class: "form-control" %>
  </div>
<% end %>